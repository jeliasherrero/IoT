	<h2>Sensores</h2>
	<a href="/properties/add?token={{user.key}}" type="submit" class="btn btn-default">Añadir nuevo sensor</a>
	<ul class="list-group">		

						{{#each sensors}}
						<hr>
						<h4><a href="/properties/{{this.id}}?token={{../user.key}}">{{name}}</a></h4>
						
						<ul class="list-group">
							{{#each this.values}}
								<li class="list-group-item">
									<span id="value-{{@key}}" class="badge">{{this}}</span>
								{{@key}}
								</li>
							{{/each}}
							{{#each this.data}}
								<li class="list-group-item">
									<span id="desc-{{@key}}" class="badge">{{this.description}}</span>
								Descripción
								</li>
								<li class="list-group-item">
									<span id="data-{{@key}}" class="badge">{{this.customFields.gpio}}</span>
								GPIO
								</li>
							{{/each}}
							</li>
						</ul>
						<a href="/properties/edit/{{this.id}}?token={{../user.key}}" type="submit" class="btn btn-default">Editar</a>
						<a href="/properties/delete/{{this.id}}?token={{../user.key}}" type="submit" class="btn btn-default">Borrar</a>
						{{/each}}

	</ul>
<h2>Actuadores</h2>
	<ul class="list-group">				
						{{#each actions}}
						<hr>
						<h4>{{name}}</h4>
						{{/each}}

	</ul>